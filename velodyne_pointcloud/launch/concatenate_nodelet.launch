<!-- -*- mode: XML -*- -->
<!-- run velodyne_pointcloud/ConcatenateNodelet in a nodelet manager -->

<launch>
  <arg name="manager" default="velodyne_nodelet_manager" />

  <node pkg="nodelet" type="nodelet" name="$(arg manager)_concatenate"
        args="load velodyne_pointcloud/ConcatenateNodelet $(arg manager)" >
  </node>
</launch>
